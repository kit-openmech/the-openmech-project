}

.block-nice-menus ul.nice-menu li a{ 
	background:transparent url('images/menu-expanded.png') no-repeat center right; 
	padding:10px 15px 10px 10px; 
	text-align: left;
}

ul.nice-menu,
ul.nice-menu ul,
ul.nice-menu li {
    border: 0;
    background: #2f2f2f;
}

ul.nice-menu-right li.menuparent,
ul.nice-menu-right li li.menuparent{
    background: #2f2f2f url('arrow-right.png') right center no-repeat; 
    text-align: left; 
