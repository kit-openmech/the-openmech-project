h2.block-title {
	display: none;
}

.block-main-menu ul.menu,
.menu-2 ul.menu {
	display:block; 
	padding:0; 
	margin:0; 
	position:relative; 
	z-index:100; 
	background-color: #2f2f2f;
}

.block-main-menu ul.menu li, 
.menu-2 ul.menu li {
	display:inline-block; 
	float:left; 
	vertical-align: middle; 
	padding:0; 
	margin:0; 
}


.header-secondary-menu ul.links li a {
	outline:none; 
	text-decoration: none; 
	margin:0 25px 0 0; 
	padding:10px 5px; 
	-weight:normal; 
	text-align:center; 
	text-transform:uppercase; 
	font-family: Myriad Pro,"Lucida Grande","Lucida Sans Unicode",Segoe UI,Helvetica,Arial,sans-serif;
	display:block; 
	background-color:transparent;
	color: #fff;
}

.block-main-menu ul.menu li a {
	font-size:14px; 
}

.menu-2 ul.menu li a { 
	font-size:10px; 
}

.block-main-menu ul.menu li.first a { 
	padding-left:0; 
}

.block-main-menu ul.menu li.last a { 
