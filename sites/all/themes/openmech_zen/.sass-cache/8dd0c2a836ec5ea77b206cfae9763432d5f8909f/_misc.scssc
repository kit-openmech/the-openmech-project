/***
 * Nice Menus
 */

.block-nice-menus {
  clear:both;
}


/* Hide nice-menu-hide-title */
.nice-menu-show-title {
  display: none;
}

.block-nice-menus ul.nice-menu {
	display:inline; 
	padding:0; 
	margin:0; 
	position:relative; 
	z-index:100; 
	background-color: #2f2f2f;
}

.block-nice-menus ul.nice-menu li {
	display:inline-block; 
	float:left; 
	vertical-align: middle; 
	padding:0; 
	margin:0; 
}

.block-nice-menus ul.nice-menu li a, 
.header__secondary-menu ul.links li a {
	outline:none; 
	text-decoration: none; 
	margin:0 15px 0 0; 
	padding:10px 5px; 
	-weight:normal; 
	text-align:center; 
	text-transform:uppercase; 
	font-family: Myriad Pro,"Lucida Grande","Lucida Sans Unicode",Segoe UI,Helvetica,Arial,sans-serif;
	//display:block; 
}

.block-nice-menus ul.nice-menu li a{
	font-size:14px; 
}

.header__secondary-menu ul.links li a {
	color:#dddee9;
	text-transform:uppercase;
	font-size:12px;
}

.block-nice-menus ul.nice-menu li a:hover,
.block-nice-menus ul.nice-menu li a:active,
.header__secondary-menu ul.links li a:hover,
.header__secondary-menu ul.links li a:active { 
